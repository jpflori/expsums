@ARTICLE{6006614,
author={Lisoněk, Petr},
journal={Information Theory, IEEE Transactions on},
title={An Efficient Characterization of a Family of Hyperbent Functions},
year={2011},
month={sept. },
volume={57},
number={9},
pages={6010 -6014},
abstract={Charpin and Gong recently characterized a large class of hyperbent functions defined on fields of order 2n, which include the well-known monomial functions with the Dillon exponent as a special case. We give a reformulation of the Charpin-Gong criterion in terms of the number of rational points on certain hyperelliptic curves. We present two applications of our result: The time needed to check the hyperbentness of a specific function is now polynomial in n , and hyperbent functions with subfield coefficients can be constructed.},
keywords={Charpin-Gong criterion;Dillon exponent;hyperbent function family;hyperelliptic curves;monomial functions;polynomial;subfield coefficients;public key cryptography;},
doi={10.1109/TIT.2011.2162171},
ISSN={0018-9448},}

